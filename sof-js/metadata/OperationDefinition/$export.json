{
    "resourceType": "OperationDefinition",
    "url": "http://sql-on-fhir.org/OperationDefinition/$export",
    "version": "0.0.1",
    "id": "$export",
    "name": "$export",
    "status": "active",
    "kind": "operation",
    "code": "export",
    "system": false,
    "type": true,
    "instance": true,
    "resource": [
      "ViewDefinition"
    ],
    "inputProfile":  "http://sql-on-fhir.org/Parameters/export-parameters",
    "outputProfile": "http://sql-on-fhir.org/Parameters/export-status",
    "parameter": [
        {
            "name": "viewCanonicalReference",
            "use": "in",
            "min": 1,
            "max": "*",
            "type": "Reference",
            "documentation": "The canonical reference to the view definition"
        },
        {
            "name": "format",
            "use": "in",
            "min": 1,
            "max": "1",
            "type": "code",
            "binding": {
                "strength": "extensible",
                "valueSet": "http://sql-on-fhir.org/ValueSet/output-format"
            },
            "documentation": "The format of the output, could be csv, parquet, json, etc."
        },
        { 
            "name": "exportId",
            "use": "out",
            "min": 0,
            "max": "1",
            "type": "string",
            "documentation": "The ID of the export"
        },
        {
            "name": "location",
            "use": "out",
            "min": 1,
            "max": "1",
            "type": "uri",
            "documentation": "The URL to poll for the status of the export"
        },
        {
            "name": "status",
            "use": "out",
            "min": 1,
            "max": "1",
            "type": "code",
            "binding": {
                "strength": "required",
                "valueSet": "http://sql-on-fhir.org/ValueSet/export-status"
            },
            "documentation": "The status of the export, could be accepted, running, completed, cancelled, failed"
        },
        {
            "name": "output",
            "use": "out",
            "min": 0,
            "max": "*",
            "part": [
                {
                    "name": "name",
                    "use": "out",
                    "min": 1,
                    "max": "1",
                    "type": "string",
                    "documentation": "The name of ViewDefinition"
                },
                {
                    "name": "location",
                    "use": "out",
                    "min": 1,
                    "max": "1",
                    "type": "uri",
                    "documentation": "The URL to poll the results of export"
                },
                {
                    "name": "format",
                    "use": "out",
                    "min": 1,
                    "max": "1",
                    "type": "code",
                    "binding": {
                        "strength": "extensible",
                        "valueSet": "http://sql-on-fhir.org/ValueSet/output-format"
                    },
                    "documentation": "The format of the output, could be csv, parquet, json, etc."
                }
            ]
        }
    ]
}